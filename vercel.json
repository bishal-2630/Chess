{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/health.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/redoc/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/chess_game/build/web/assets/$1"
    },
    {
      "src": "/canvaskit/(.*)",
      "dest": "/chess_game/build/web/canvaskit/$1"
    },
    {
      "src": "/flutter.js",
      "dest": "/chess_game/build/web/flutter.js"
    },
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/chess_game/build/web/flutter_bootstrap.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/chess_game/build/web/main.dart.js"
    },
    {
      "src": "/favicon.png",
      "dest": "/chess_game/build/web/favicon.png"
    },
    {
      "src": "/manifest.json",
      "dest": "/chess_game/build/web/manifest.json"
    },
    {
      "src": "/",
      "dest": "/chess_game/build/web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/chess_game/build/web/index.html"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
